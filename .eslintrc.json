{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-unused-vars" :"off",
        "no-constant-condition":"off",
        "semi":"error",
        "quotes": ["error", "double"]
    }
}
